class Solution {
public:
    int findTheWinner(int n, int k) {
        queue<int>q;
        for(int i=1; i<=n; i++) {
            q.push(i);
        }
        while(q.size() > 1) {
            for(int i=1; i<k; i++) {
               int n = q.front();
                q.pop();
                q.push(n);
            }
            q.pop();

        }
        int num = q.front();
        return num;
    }
};
